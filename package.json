{
  "name": "signage",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "bundle": "npm run clean && npm run build-only && node bundle.js",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "build-only": "vite build --base=/Signage/",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@shimyshack/uid": "^0.1.9",
    "buffer": "^6.0.3",
    "dateformat": "^5.0.3",
    "debounce": "^2.0.0",
    "idb-keyval": "^6.2.1",
    "libsodium-wrappers": "^0.7.13",
    "magic-bytes.js": "^1.10.0",
    "mime": "^4.0.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.51.0",
    "should": "^13.2.3",
    "usemodal-vue3": "^0.3.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.1",
    "@types/dateformat": "^5.0.2",
    "@types/debounce": "^1.2.4",
    "@types/node": "^20.12.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.74.1",
    "typescript": "~5.4.4",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.10",
    "archiver": "^7.0.1"
